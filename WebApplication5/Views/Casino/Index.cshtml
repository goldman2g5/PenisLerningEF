<h1>Очки: @ViewBag.Count</h1>

@{  
    List<string> GetImages()
    {
        var result = new List<string>();
        for (int i = 1; i < 10; i++)
        {
            result.Add(new Random().Next(1, 3) == 1 ? "/lib/joint.png" : "/lib/tnt.png");
        }
        return result;
    }
}


<div class="grid">
    
    @foreach (var item in GetImages())
    {
        <div class="square"><img src=@item onclick="sendData(1)" /></div>
    }

</div>

